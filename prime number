#include<stdio.h>

int main()
{   
    int num,i,flag=0;
    scanf("%d",&num);
    if(num<2)
    {
        printf("0");
        return 0;
    }
    for(i=2;i<=num/2;i++) {
        if(num%i==0) {
            flag=1;
            break;
        }
    }
    if(flag==0){
        printf("1");
    }else
    {
        printf("0");
    }
    return 0;
}

